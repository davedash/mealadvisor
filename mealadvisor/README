# to migrate from sf version

    REPLACE INTO 
      auth_user

    SELECT 
      p.id AS id, 
      replace(username, 'http://', '') AS username,
      null AS first_name,
      null AS last_name,
      email,
      concat(algorithm,'$',salt,'$',password),
      0 as is_staff,
      1 as is_active,
      0 as is_superuser, 
      last_login,
      u.created_at as date_joined


    FROM 
      `sf_guard_user` u, profile p 

    WHERE
      p.userid = u.id

